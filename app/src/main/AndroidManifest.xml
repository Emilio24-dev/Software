<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.registro">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/snowfox"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/snowfox_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Registro"
        tools:targetApi="31">
        <activity
            android:name=".SettActivity"
            android:exported="false" />
        <activity
            android:name=".CasaActivity"
            android:exported="false" />
        <activity
            android:name=".PerfilActivity"
            android:exported="false" />
        <activity
            android:name=".Inicio"
            android:exported="false" /> <!-- @mipmap/ic_launcher_round -->
        <!-- Actividad principal: Login -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- Actividad registro -->
        <activity
            android:name=".registro"
            android:exported="true" />
    </application>

</manifest>